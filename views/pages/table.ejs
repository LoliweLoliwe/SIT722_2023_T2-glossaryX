<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head'); %>
</head>
<body class="container">

<header>
    <%- include('../partials/header'); %>
</header>

<main>
    <table class="fixed">
        <tr>
            <th> # </th>
            <th> Term </th>
            <th> Description </th>
            <th> Reference </th>
        </tr>
    
        <% for(var i=0; i<table.length; i++) { %>
            <tr>    
                <td> <%= table[i].number %> </td>
                <td> <%= table[i].term %> </td>
                <td> <%= table[i].description %> </td>
                <td> <%= table[i].reference %> </td>
            </tr> 
        <% } %>
    
    </table>
</main>

<footer>
    <%- include('../partials/footer'); %>
</footer>

</body>
</html>